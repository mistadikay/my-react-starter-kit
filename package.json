    "normalize-css": "^2.3.1",
    "preact": "^8.1.0",
    "preact-compat": "^3.14.3",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-redux": "^5.0.1",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.0.0",
    "redux-actions": "^2.0.1",
    "redux-saga": "^0.14.3",
    "reselect": "^3.0.0",
    "shortid": "^2.2.6",
    "string-template": "^1.0.0",
    "url-join": "^2.0.1",
    "whatwg-fetch": "^2.0.0"
  },
