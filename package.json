{
  "name": "react-starter-kit",
  "version": "0.0.0",
  "description": "Yet another React starter kit for my personal use. You can use it as well. Or not.",
  "main": "build/index.js",
  "scripts": {
    "lint:js": "eslint .",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint:flow": "flow check",
    "lint": "npm-run-all -p lint:*",
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "clean": "rimraf build",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack",
    "test:unit": "cross-env BABEL_DISABLE_CACHE=1 jest --config jest.config.json --coverage --no-cache",
    "tdd:unit": "cross-env BABEL_DISABLE_CACHE=1 jest --config jest.config.json --coverage --watch --no-cache",
    "pretest": "npm-run-all -s lint build",
    "test": "npm run test:unit",
    "postinstall": "flow-typed install",
    "prepush": "npm test"
  },
  "repository": "mistadikay/react-starter-kit",
  "author": "Denis Koltsov <iam@mistadikay.com> (https://mistadikay.com/)",
  "homepage": "https://github.com/mistadikay/react-starter-kit",
  "license": "MIT",
  "bugs": "https://github.com/mistadikay/react-starter-kit/issues",
